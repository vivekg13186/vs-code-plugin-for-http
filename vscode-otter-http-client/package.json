{
  "name": "vscode-otter-http-client",
  "displayName": "vscode-otter-http-client",
  "description": "",
  "version": "0.0.3",
  "publisher": "vivekopd",
  "icon": "media/otter.png",
  "repository": {
    "url": "https://github.com/vivekg13186/vs-code-plugin-for-http"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "otter.http.tree.reload",
        "title": "Refresh",
        "icon": {
          "light": "media/light/refresh.svg",
          "dark": "media/dark/refresh.svg"
        }
      },
      {
        "command": "otter.http.new.testcase",
        "title": "Add new testcase",
        "category": "HTTP Client",
        "icon": {
          "light": "media/light/plus.svg",
          "dark": "media/dark/plus.svg"
        }
      },
      {
        "command": "otter.http.delete.testcase",
        "title": "Remove testcase",
        "category": "HTTP Client",
        "icon": {
          "light": "media/light/trash.svg",
          "dark": "media/dark/trash.svg"
        }
      },
      {
        "command": "otter.http.edit.testcase",
        "title": "Edit testcase",
        "category": "HTTP Client",
        "icon": {
          "light": "media/light/edit.svg",
          "dark": "media/dark/edit.svg"
        }
      }
    ],
    "views": {
      "httpexplorer": [
        {
          "id": "httpexplorer",
          "name": "HTTP request",
          "contextualTitle": "Test Scripts"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "httpexplorer",
          "icon": "media/dark/otter.svg",
          "title": "HTTP Request Explorer"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "otter.http.new.testcase",
          "when": "view == httpexplorer",
          "group": "navigation"
        },
        {
          "command": "otter.http.tree.reload",
          "when": "view == httpexplorer",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "otter.http.delete.testcase",
          "when": "view == httpexplorer && viewItem == httprequest"
        },
        {
          "command": "otter.http.edit.testcase",
          "when": "view == httpexplorer && viewItem == httprequest"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/coffeescript": "^2.5.7",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "coffeescript": "^2.7.0",
    "js-interpreter": "^5.1.2",
    "level": "^8.0.1"
  }
}
